{
	"name": "flarum-lang/esperanto",
	"type": "flarum-extension",
	"description": "Esperanto language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/esperanto",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"eo-locale",
		"esperanto-locale",
		"esperanto"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/esperanto/issues",
		"source": "https://github.com/flarum-lang/esperanto"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Esperanto",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "eo",
			"title": "Esperanto"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
